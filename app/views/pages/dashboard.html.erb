<div class="container">
  <h1>Dashboard</h1>
  <div>
  <%= simple_form_for(@session) do |f| %>
    <%= f.input :file %>
    <%= f.input :file_cache, as: :hidden %>
    <%= f.button :submit, "Convert", class: "btn-primary" %>
    <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
  <% end %>
  </div>
</div>
